{
  "compilerOptions": {
    "moduleResolution": "node",
    "target": "es5",
    "downlevelIteration": true,
    "module": "es2015",
    "lib": [
      "es2015",
      "es2016",
      "es2017",
      "dom"
    ],
    "strict": true,
    "declaration": true,
    "allowSyntheticDefaultImports": true
  },
  "exclude": [
    "node_modules",
  ]
}
